<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="NopCommerce">
  	
	<test name="run on headless Firefox">
	<parameter name="browser" value="h_firefox" ></parameter>
		<classes>
			<class name="com.nopcommerce.user.Level_04_Multiple_Browser"/>
		</classes>
  	</test>	 
  	
	<test name="run on head less Chrome">
	<parameter name="browser" value="h_chrome" ></parameter>
		<classes>
			<class name="com.nopcommerce.user.Level_04_Multiple_Browser"/>
		</classes>
  	</test>	 

  	
</suite >